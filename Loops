		                           char value2 = 'mohan';
		                              if(value1 == value2)  {
	                                  	System.out.println("The Characters are equal");
	                                	}
		                                 else if(value != value2) {
		                                       System.out.println("The characters are not equal");
		                                          }
	                                      }
    4Q.       Write a program to print the odd and even numbers ?
    Ans.      public static void main(String[] args)  {
                    int num = 25;
                    if (num % 2 == 0)  {
                      System.out.println("This is an even number:",+num);
                    }
                  else {
                      System.out.println(" This is Odd number:",+num);
                     }
                   }
    5Q.        Write a program to print largest number among three numbers ?
    Ans.             public static void main(String[] args) {
                        int x = 45, y = 24, z = 52;
                            if( x >= y && x >= z)
                                 System.out.println(x + " is the largest number.");
                            else if (y >= x && y >= z)
                                 System.out.println(y + " is the largest number.");
                            else
                                 System.out.println(z + " is the largest number.");
                                          }
                                }
     6Q.        Write a program to print even number between 10 and 100 using while ?
     Ans.                public static void main(String args[])   {  
                              int i = 1, n = 100;  
                        System.out.print("List of even numbers from 1 to 100");  
                           while (i<=n)  { 
                             if ( i%2 == 0 )  {
                           System.out.print(i + " ");
                              }
                                i++;
                            }
                         }  
      7Q.       Write a program to print 1 to 10 using the do-while loop statement ?
      Ans.            public static void main(String[] args)  {
                                    int x = 1;
                                    do {
                                    System.out.println(x);
                                    x++;
                                   } while(x<=10);
                                  }
                                }
      8Q.       Write a program to find Armstrong number or not ?
      Ans.              public static void main(String[] args)  {
                        int n, r = 0, n1;
                        n1 = n;
                        while (n > 0)  {
                        r = r + Pow(n % 10, 3);
                        n = n / 10;
                      }
                        if (n1 == r)
                        System.out.println("Armstrong");
                        else {
                        System.out.println("Not armstrong");
                      }
                   }
     9Q.        Write a program to find the prime or not ?
     Ans.          public static void main(String[] args)  {
                        int x, p = 0, i =1;
                      while(i <= x)  {
                      if (x % i == 0)  {
                            p++;
                          }
                          i++;
                        }
                        if (p == 2)
                        System.out.println("Prime");
                          else 
                          System.out.println("Not a prime");
                         }
     10Q.       Write a program to palindrome or not ?
     Ans.              public static void main(String[] args)  {
                            int n, r = 0, p;
                            System.out.println("Enter any number");
                          DataInputStream obj = new DataInputStream(System.in);
                          n = Integer.parseInt(obj.readLine());
                          p = n;
                           while (n > 0)  {
                           r = (r * 10) + (n % 10);
                           n = n / 10;
                          }
                            if ( p == r)
                            System.out.println("polindrome");
                              else 
                              System.out.println("Not a Polindrome");
                             }
      11Q.        Program to check whether a number is EVEN or ODD using switch ?
      Ans.                public static void main(String[] args)  {
                                int n = 100;
                                switch (n % 2)  {
                                  case 0:
                                    System.out,println(n + " is an Even number");
                                    break;
                                  case 1:
                                    System.out.println(n + is an Odd Number");
                                  }
                                }
      12Q.        Print gender (Male/Female) program according to given M/F using switch ?
      Ans.                public static void main(String[] args)  {
                              char gender;
                              System.out.printf("Enter gender (M/m or F/f): ");
                              switch (gender) {
                                 case 'M':
                                 case 'm':
                                 System.out.printf("Male.");
                                 break;

                                case 'F':
                                case 'f':
                                System.out.printf("Female.");
                              }
                            System.out.println("\n");
                          }
                      }
      13Q.          Program for multiple if else statement(Largest number in 10,20 and 30) ?
                              public static void main(String[] args)  {
                                int x = 10, y = 20, z = 30;
                                  if (x>y && x>z)  {
                                  System.out.println("Largest Number is:",+x);
                                }
                                else  if (y>x && y>z)  {
                                System.out.println(""Largest Number is:",+y);
                               }  
                                 else  {
                                     System.out.println("Largest Number is:",+z);
                                    }
                                }                     
                      
